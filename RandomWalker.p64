--[[pod_format="raw",created="2024-10-02 15:06:36",modified="2024-10-02 16:11:02",revision=226]]

function Walker()
	Walker = {xPosition = 50, yPosition = 50}
	return Walker
end

function step()
	choice = flr(rnd(4))
	
	if choice == 0 then
		Walkers[1].xPosition += 1
	elseif choice == 1 then
		Walkers[1].xPosition -= 1
	elseif choice == 2 then
		Walkers[1].yPosition += 1
	else 
		Walkers[1].yPosition -= 1
	end
end
:: main.lua
--[[pod_format="raw",created="2024-10-02 14:56:10",modified="2024-10-02 16:11:02",revision=263]]
include "Walker.lua"

Walkers = {}
function _init()
	add(Walkers,Walker())
end

function _update()
	step()
end

function _draw()
	circfill(Walkers[1].xPosition, Walkers[1].yPosition, 1, 10)
end
